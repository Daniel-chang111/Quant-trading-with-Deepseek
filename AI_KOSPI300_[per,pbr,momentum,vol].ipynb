                'PER': per,
                'PBR': pbr,
                'ëª¨ë©˜í…€_1m': momentum_1m,
                'ëª¨ë©˜í…€_3m': momentum_3m,
                'ë³€ë™ì„±': volatility,
                '1ì›”31ì¼_ê°€ê²©': df_price['ì¢…ê°€'].iloc[-1]
