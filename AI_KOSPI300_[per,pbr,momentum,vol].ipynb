def fetch_ticker_data(ticker, date):
    """
    í•˜ë‚˜ì˜ ì¢…ëª©ì— ëŒ€í•œ ë°ì´í„° ìˆ˜ì§‘
    - ìºì‹œ í™•ì¸ â†’ ìˆìœ¼ë©´ ìºì‹œ ì‚¬ìš©, ì—†ìœ¼ë©´ ìƒˆë¡œ ìˆ˜ì§‘
    - ìˆ˜ì§‘ í•­ëª©: PER, PBR, ëª¨ë©˜í…€(1,3,6ê°œì›”), ë³€ë™ì„±, í˜„ì¬ê°€
    
    Args:
        ticker: ì¢…ëª© ì½”ë“œ
        date: ê¸°ì¤€ ë‚ ì§œ
    Returns:
        result ë”•ì…”ë„ˆë¦¬ (ì„±ê³µ ì‹œ), None (ì‹¤íŒ¨ ì‹œ)
    """
    
    # 1. ìºì‹œ í™•ì¸
    cached = cache.load(ticker, date)
    if cached:
        return cached  # ìºì‹œì— ìˆìœ¼ë©´ ë°”ë¡œ ë°˜í™˜

    try:
        # 2. ì¢…ëª©ëª… ì¡°íšŒ
        name = stock.get_market_ticker_name(ticker)

        # 3. PER, PBR ë°ì´í„° ì¡°íšŒ
        # get_market_fundamental: ì¬ë¬´ì œí‘œ ê¸°ë°˜ ë°ì´í„°
        df_fund = stock.get_market_fundamental(date, date, ticker)
        if df_fund.empty:
            return None

        fund_row = df_fund.iloc[0]  # ì²« ë²ˆì§¸ í–‰ (í•´ë‹¹ ë‚ ì§œ)
        per = fund_row['PER'] if 'PER' in fund_row.index else np.nan
        pbr = fund_row['PBR'] if 'PBR' in fund_row.index else np.nan

        # PERì´ ìœ íš¨í•˜ì§€ ì•Šìœ¼ë©´ ì œì™¸
        if pd.isna(per) or pd.isna(pbr) or per <= 0:
            return None

        # 4. ê°€ê²© ë°ì´í„° ì¡°íšŒ (ëª¨ë©˜í…€, ë³€ë™ì„± ê³„ì‚°ìš©)
        # 200ì¼ ì „ ë‚ ì§œ ê³„ì‚°
        start_date = (datetime.strptime(date, '%Y%m%d') - timedelta(days=200)).strftime('%Y%m%d')
        # get_market_ohlcv_by_date: ì¼ë³„ OHLCV ë°ì´í„° (Open, High, Low, Close, Volume)
        df_price = stock.get_market_ohlcv_by_date(start_date, date, ticker)

        if len(df_price) < 100:  # ë°ì´í„°ê°€ ë„ˆë¬´ ì ìœ¼ë©´ ì œì™¸
            return None

        # 5. ëª¨ë©˜í…€ ê³„ì‚°
        # ëª¨ë©˜í…€ = (í˜„ì¬ê°€ / ê³¼ê±°ê°€ - 1) * 100 (ë°±ë¶„ìœ¨)
        # iloc[-1]: ë§ˆì§€ë§‰ í–‰(ìµœì‹ ), iloc[-22]: 22ì¼ ì „(ì•½ 1ê°œì›”)
        momentum_1m = (df_price['ì¢…ê°€'].iloc[-1] / df_price['ì¢…ê°€'].iloc[-22] - 1) * 100
        momentum_3m = (df_price['ì¢…ê°€'].iloc[-1] / df_price['ì¢…ê°€'].iloc[-66] - 1) * 100
        # 132ì¼ ì „(ì•½ 6ê°œì›”) ë°ì´í„°ê°€ ìˆìœ¼ë©´ ê³„ì‚°
        momentum_6m = (df_price['ì¢…ê°€'].iloc[-1] / df_price['ì¢…ê°€'].iloc[-132] - 1) * 100 if len(df_price) > 132 else 0
        
        # 6. ë³€ë™ì„± ê³„ì‚° (ìµœê·¼ 60ì¼ ì¼ê°„ ìˆ˜ìµë¥ ì˜ í‘œì¤€í¸ì°¨)
        volatility = df_price['ë“±ë½ë¥ '].tail(60).std()

        # 7. ê²°ê³¼ ë”•ì…”ë„ˆë¦¬ ìƒì„±
        result = {
            'í‹°ì»¤': ticker,
            'ì¢…ëª©': name,
            'PER': round(per, 2),
            'PBR': round(pbr, 2),
            'ëª¨ë©˜í…€_1m': round(momentum_1m, 2),
            'ëª¨ë©˜í…€_3m': round(momentum_3m, 2),
            'ëª¨ë©˜í…€_6m': round(momentum_6m, 2),
            'ë³€ë™ì„±': round(volatility, 2),
            'í˜„ì¬ê°€': df_price['ì¢…ê°€'].iloc[-1]
        }

        # 8. ìºì‹œ ì €ì¥
        cache.save(ticker, date, result)
        return result

    except Exception as e:
        # ì˜ˆì™¸ ë°œìƒ ì‹œ None ë°˜í™˜ (í•´ë‹¹ ì¢…ëª© ê±´ë„ˆëœ€)
        return None


